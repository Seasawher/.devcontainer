{
    "name": "${containerWorkspaceFolder}",
    "dockerComposeFile": "./docker-compose.yml",
    "service": "devcontainer",
    "workspaceFolder": "/workspace",
    "customizations": {
        "vscode": {
            "settings": {
                "editor.formatOnSave": true,
                "editor.insertSpaces": true,
                "files.eol": "\n",
                "files.trimTrailingWhitespace": true
            },
            "extensions": []
        }
    }
}